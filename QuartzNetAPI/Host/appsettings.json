{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Quartz": {
    "quartz.scheduler.instanceName": "bennyScheduler",
    "quartz.scheduler.instanceId": "AUTO",
    "quartz.threadPool.threadCount": "10",

    "quartz.serializer.type": "json",
    "quartz.jobStore.type": "Quartz.Impl.AdoJobStore.JobStoreTX, Quartz",
    "quartz.jobStore.driverDelegateType": "Quartz.Impl.AdoJobStore.StdAdoDelegate, Quartz",
    "quartz.jobStore.tablePrefix": "QRTZ_",
    "quartz.jobStore.dataSource": "default",
    "quartz.jobStore.useProperties": "false", //#JobDataMaps是否都为String类型",jobDataMap需要存储日志信息，所以必须设置false
    "quartz.dataSource.default.connectionString": "Data Source=File/sqliteScheduler.db;",
    "quartz.dataSource.default.provider": "SQLite-Microsoft"
  },
  "AllowedHosts": [ "*" ]
}

